{
  "all": true,
  "include": [
    "index.js",
    "util/**/*.js"
  ],
  "exclude": [
    "tests/**",
    "node_modules/**",
    "coverage/**",
    ".nyc_output/**"
  ],
  "reporter": [
    "html",
    "text",
    "lcov"
  ],
  "check-coverage": true,
  "lines": 85,
  "functions": 85,
  "branches": 80,
  "statements": 85,
  "watermarks": {
    "lines": [80, 95],
    "functions": [80, 95],
    "branches": [80, 95],
    "statements": [80, 95]
  }
}