{
  "include": [
    "dist/**/*.js"
  ],
  "exclude": [
    "dist/**/*.d.ts",
    "dist/**/*.map",
    "tests/**",
    "node_modules/**",
    "coverage/**",
    ".nyc_output/**"
  ],
  "reporter": [
    "html",
    "text",
    "lcov"
  ],
  "check-coverage": true,
  "lines": 90,
  "functions": 85,
  "branches": 80,
  "statements": 90,
  "watermarks": {
    "lines": [80, 95],
    "functions": [80, 95],
    "branches": [80, 95],
    "statements": [80, 95]
  },
  "source-map": true
}
